{
  "name": "veo3-automation-tool",
  "version": "1.0.0",
  "description": "Desktop automation tool for VEO3 Flow video generation using Chrome profiles",
  "main": "src/main/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "npm run build-renderer && electron . --dev",
    "build": "vite build && electron-builder",
    "build-renderer": "vite build",
    "test": "jest",
    "postinstall": "electron-builder install-app-deps"
  },
  "keywords": [
    "electron",
    "automation",
    "veo3",
    "video",
    "chrome"
  ],
  "author": "VEO3 Automation Team",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "jest": "^29.7.0",
    "@vitejs/plugin-react": "^4.2.1",
    "vite": "^5.0.8",
    "concurrently": "^8.2.2",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "selenium-webdriver": "^4.15.0",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "fs-extra": "^11.2.0",
    "winston": "^3.11.0",
    "uuid": "^9.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.4.7"
  },
  "build": {
    "appId": "com.veo3.automation-tool",
    "productName": "VEO3 Automation Tool",
    "directories": {
      "output": "dist"
    },
    "files": [
      "src/**/*",
      "node_modules/**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    }
  }
}
