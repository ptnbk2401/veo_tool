{
  "automation": {
    "pollInterval": 5000,
    "timeout": 600000,
    "parallel": 1,
    "retryAttempts": 3,
    "headless": false
  },
  "paths": {
    "outputFolder": "./output",
    "logsFolder": "./logs",
    "backupsFolder": "./backups",
    "profilesFolder": "./profiles"
  },
  "ui": {
    "theme": "light",
    "autoSave": true,
    "notifications": true,
    "logLevel": "info"
  },
  "browser": {
    "windowSize": {
      "width": 1280,
      "height": 720
    },
    "userAgent": null,
    "downloadTimeout": 60000,
    "pageLoadTimeout": 30000
  },
  "security": {
    "allowInsecureConnections": false,
    "validateCertificates": true
  },
  "performance": {
    "maxConcurrentJobs": 3,
    "memoryLimit": 1024,
    "cleanupInterval": 3600000
  }
}
